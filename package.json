{
  "name": "react-cheat-sheet",
  "version": "0.1.0",
  "private": true,
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "jest-runner-tslint": {
    "cliOptions": {}
  },
  "dependencies": {
    "@99/helper": "0.3.0",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "match-sorter": "6.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "7.0.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-syntax-highlighter": "15.4.4",
    "react-transition-group": "4.4.2",
    "react-virtual": "2.8.2",
    "recoil": "0.4.1",
    "remark-gfm": "3.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "workerize": "0.1.8",
    "workerize-loader": "1.3.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && compress-cra",
    "eject": "react-app-rewired eject",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "lint": "jest --config etc/jest/jest.lint.js",
    "test:server": "jest --config etc/jest/jest.server.js",
    "test:dom": "jest --config etc/jest/jest.client.js",
    "eslint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook --disable-emoji || true",
      "pre-commit": "sh etc/git-hooks/check-code.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/react-router-dom": "5.3.1",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/react-transition-group": "4.4.4",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "commitizen": "4.2.3",
    "compress-create-react-app": "1.1.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-require-jsdoc-except": "1.5.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-exports": "0.3.2",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "git-cz": "4.7.6",
    "husky": "4.3.8",
    "is-ci-cli": "2.2.0",
    "jest": "27.3.1",
    "jest-css-modules-transform": "4.3.0",
    "jest-environment-jsdom": "27.3.1",
    "jest-environment-node": "27.3.1",
    "jest-runner-eslint": "1.0.0",
    "jest-runner-tslint": "2.0.23",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "10.5.4",
    "node-sass": "^5",
    "prettier": "2.2.1",
    "react-app-rewired": "2.1.8",
    "stylelint": "13.9.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-webpack-plugin": "2.1.1",
    "ts-jest": "27.0.7"
  }
}
